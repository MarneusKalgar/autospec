/*******************SVGICON******************/
.svgicon
	position relative
	&::before
		content ''
		position absolute
		top 0
		left 0
		background-size cover

	&--person
		&::before
			background url("data:image/svg+xml;base64,") 0 0 no-repeat

	&--envelope
		&::before
			background url("data:image/svg+xml;base64,") 0 0 no-repeat

	&--pencil
		&::before
			background url("data:image/svg+xml;base64,") 0 0 no-repeat

	&--phone
		&::before
			background url("data:image/svg+xml;base64,") 0 0 no-repeat

	&--location
		&::before
			background url("data:image/svg+xml;base64,") 0 0 no-repeat