include ../logo/logo
include ../menu-btn/menu-btn

mixin header-services
	+b.HEADER.header
		// block logo
		+logo({
			"id": "logo",
			"class": "logo",
			"href": "#",
			"set": "img/logo/autospec-logo-dark@2.png",
			"src": "img/logo/autospec-logo-dark.png",
			"alt": "логотип компании автоспецсервис"
		})
		// end block logo

		// block menu-btn
		+menu-btn()
		// end block menu-btn